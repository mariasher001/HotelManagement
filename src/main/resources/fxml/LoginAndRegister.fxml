               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="35.0" layoutY="168.0" text="Email" textFill="#dde010">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="34.0" layoutY="209.0" text="Password" textFill="#dde010">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="35.0" layoutY="247.0" text="Key" textFill="#dde010">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
