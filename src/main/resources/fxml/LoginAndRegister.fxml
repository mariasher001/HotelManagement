            <TextField fx:id="registerUsernameTextField" layoutX="123.0" layoutY="127.0" opacity="0.92" prefHeight="26.0" prefWidth="284.0" promptText="User-Name" style="-fx-background-color: #edca98; -fx-text-fill: #48230a;">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </TextField>
            <PasswordField fx:id="registerEmailTextField" layoutX="123.0" layoutY="168.0" prefHeight="26.0" prefWidth="284.0" promptText="xyz@gmail.com" style="-fx-background-color: #edca98; -fx-text-fill: #48230a;">
               <font>
