<AnchorPane prefHeight="650.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.marry.hotelmanagement.Controllers.LoginAndRegisterController">
   <children>
      <ImageView fitHeight="660.0" fitWidth="919.0" layoutX="-61.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/2197_ho_00_p_1024x768.jpg" />
         </image>
      </ImageView>
      <AnchorPane fx:id="LoginAnchorPane" layoutX="153.0" layoutY="132.0" prefHeight="404.0" prefWidth="536.0">
         <children>
            <ImageView fitHeight="404.0" fitWidth="536.0" layoutX="-14.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/toppng.com-brown-background-texture-2048x1536.jpg" />
               </image>
            </ImageView>
            <Label layoutX="174.0" layoutY="14.0" text="Login Page" textFill="#dde010">
